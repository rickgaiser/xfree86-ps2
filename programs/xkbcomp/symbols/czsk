partial default alphanumeric_keys 
xkb_symbols "us_sk_qwerty" {
    Name[Group1] = "US/ASCII";
    Name[Group2] = "Slovak";

    include "czsk(def_basic)"
    include "czsk(def_us_czsk)"
    include "czsk(def_us_sk_prog)"
    include "czsk(def_us_sk)"
};

