partial alphanumeric_keys 
xkb_symbols "def_qwertz_us" {
    key <AD06> {
	[               z,	        Z	],
	[               y,              Y	]
	};
    key <AB01> {
	[               y,	        Y	],
	[               z,              Z	]
	};
};

partial alphanumeric_keys 
